#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules/astro/node_modules:/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules:/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules/astro/node_modules:/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules:/mnt/c/Users/ctw03345/source/repos/scout-toolkit-v2/devlog/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.13.4_@types+node@24_4bbabd1adb2e0054482afa9a6fe9d97d/node_modules/astro/astro.js" "$@"
fi
